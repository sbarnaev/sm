CREATE TABLE `#PREFIX#orders` (
  `order_id` int(11) NOT NULL AUTO_INCREMENT,
  `product_id` int(11) DEFAULT NULL COMMENT 'ID продукта, нужен для апселла',
  `order_date` int(11) NOT NULL,
  `payment_date` int(11) DEFAULT NULL,
  `summ` int(11) DEFAULT NULL,
  `client_name` varchar(50) NOT NULL,
  `client_email` varchar(50) NOT NULL,
  `client_phone` varchar(50) DEFAULT NULL,
  `client_city` varchar(255) DEFAULT NULL,
  `client_address` varchar(255) DEFAULT NULL,
  `client_index` varchar(8) DEFAULT NULL,
  `client_comment` text,
  `admin_comment` text,
  `payment_id` int(11) DEFAULT NULL COMMENT 'id системы оплаты',
  `ship_method_id` int(11) DEFAULT NULL COMMENT 'метод доставки',
  `ship_status` varchar(20) DEFAULT NULL COMMENT 'статус доставки',
  `sale_id` int(11) DEFAULT NULL,
  `partner_id` int(11) DEFAULT NULL,
  `status` int(11) NOT NULL COMMENT '1- оплачен, 0 - неоплчен, 2 - требует проверки, 7 - подтверждение доставки по емейл, 9 - возврат',
  `base_id` int(11) DEFAULT NULL COMMENT 'id продукта кому писать конверсию',
  `visit_param` text COMMENT 'параметры визита',
  `channel_id` int(11) DEFAULT NULL COMMENT 'id рекламного канала',
  `dwl_time` int(11) DEFAULT NULL COMMENT 'время начала скачивания, 0 - ещё не скачано',
  `ip` varchar(15) DEFAULT NULL,
  `remind_letter` int(11) NOT NULL DEFAULT '0' COMMENT 'кол-во отосланных писем дожимов',
  `order_info` text,
  `remind_sms` tinyint(4) NOT NULL DEFAULT '0',
  `installment_map_id` int(11) NOT NULL DEFAULT '0' COMMENT 'связь с id действующей рассрочки',
  `utm` text,
  `is_recurrent` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'повторный заказ',
  `subs_id` int(11) DEFAULT '0' COMMENT 'ID подписки для продления',
  PRIMARY KEY (`order_id`),
  UNIQUE KEY `order_date` (`order_date`),
  KEY `order_date_2` (`order_date`)
) ENGINE=InnoDB AUTO_INCREMENT=47 DEFAULT CHARSET=utf8;



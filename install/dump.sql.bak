CREATE TABLE `#PREFIX#acl` (
  `acl_id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `permissions` text,
  PRIMARY KEY (`acl_id`),
  KEY `user_id` (`user_id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#action_log` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `extension` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT 'расширение: курсы, блог и т.д.',
  `method` varchar(50) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT 'новый, редактировать, удалить',
  `item` varchar(50) COLLATE utf8mb4_unicode_ci DEFAULT NULL COMMENT 'тип объекта: пост, категория, заказ и т.д',
  `item_id` int(11) NOT NULL DEFAULT '0',
  `date` int(11) NOT NULL,
  `user_id` int(11) NOT NULL DEFAULT '0',
  `data` text COLLATE utf8mb4_unicode_ci,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=12 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

CREATE TABLE `#PREFIX#aff_partner_data` (
  `user_id` int(11) NOT NULL,
  `hits` int(11) NOT NULL,
  `requsits` varchar(255) DEFAULT NULL,
  `ref_id` int(11) NOT NULL DEFAULT '0' COMMENT 'Id от кого пришёл',
  `custom_comiss` int(11) DEFAULT NULL COMMENT 'индивид. комиссия партнёра',
  `postbacks` longtext COMMENT 'постбеки для партнёрки',
  `fb_pixel` text,
  PRIMARY KEY (`user_id`),
  KEY `userid` (`user_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#aff_short_links` (
  `link_id` int(11) NOT NULL AUTO_INCREMENT,
  `partner_id` int(11) NOT NULL,
  `url` varchar(255) NOT NULL,
  `link_desc` varchar(255) DEFAULT NULL,
  `date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`link_id`),
  KEY `partner_id` (`partner_id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;


CREATE TABLE `#PREFIX#aff_stat_hits` (
  `partner_id` int(11) NOT NULL,
  `date` date NOT NULL,
  `hits` int(11) NOT NULL,
  PRIMARY KEY (`partner_id`,`date`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#aff_transaction` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `order_id` int(11) DEFAULT NULL,
  `product_id` int(11) NOT NULL COMMENT 'id продукта',
  `summ` int(11) NOT NULL,
  `pay` int(11) NOT NULL COMMENT 'выплачено ',
  `type` int(11) NOT NULL,
  `date` int(11) NOT NULL,
  `client_email` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `userid` (`user_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#amocrm` (
  `lead_id` int(11) NOT NULL,
  `contact_id` int(11) NOT NULL,
  `order_id` int(11) NOT NULL,
  `statistics_data` text,
  `partners_data` text,
  `installment_map_id` int(11) DEFAULT NULL,
  PRIMARY KEY (`lead_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#author_transaction` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `order_id` int(11) DEFAULT NULL,
  `product_id` int(11) DEFAULT NULL,
  `summ` int(11) NOT NULL,
  `pay` int(11) NOT NULL,
  `type` int(11) NOT NULL,
  `date` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `userid` (`user_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#blog_posts` (
  `post_id` int(11) NOT NULL AUTO_INCREMENT,
  `rubric_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `alias` varchar(255) NOT NULL,
  `title` varchar(255) NOT NULL,
  `meta_desc` text,
  `meta_keys` text,
  `post_img` varchar(255) DEFAULT NULL,
  `img_alt` varchar(255) DEFAULT NULL,
  `intro` text,
  `text` longtext,
  `start_date` int(11) NOT NULL,
  `end_date` int(11) NOT NULL,
  `create_date` int(11) NOT NULL,
  `hits` int(11) NOT NULL,
  `status` int(11) NOT NULL,
  `custom_code` text,
  `show_cover` int(11) NOT NULL DEFAULT '1' COMMENT 'показывать обложку при просмотре',
  `author_id` mediumint(9) DEFAULT NULL,
  `sort` smallint(6) NOT NULL DEFAULT '0',
  PRIMARY KEY (`post_id`),
  KEY `rubric` (`rubric_id`)
) ENGINE=InnoDB AUTO_INCREMENT=13 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#blog_rubrics` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `alias` varchar(255) NOT NULL,
  `title` varchar(255) NOT NULL,
  `access_type` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'тип доступа 0 - не выбран, 1 - по группе, 2 - по подписке',
  `groups` text,
  `planes` text,
  `meta_desc` text,
  `meta_keys` text,
  `short_desc` text,
  `create_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `status` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#channels` (
  `channel_id` int(11) NOT NULL AUTO_INCREMENT,
  `group_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `source` varchar(100) DEFAULT NULL,
  `medium` varchar(100) DEFAULT NULL,
  `campaign` varchar(100) DEFAULT NULL,
  `content` varchar(100) DEFAULT NULL,
  `term` varchar(100) DEFAULT NULL,
  `utm` varchar(255) NOT NULL COMMENT 'склеенная utm метка ',
  `summ` int(11) NOT NULL,
  `channel_desc` text,
  `create_date` int(11) NOT NULL,
  `hits` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`channel_id`),
  KEY `utm` (`utm`),
  KEY `group` (`group_id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#channels_group` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `create_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#conditions` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `type` smallint(6) NOT NULL,
  `value_xx` int(11) DEFAULT NULL,
  `status` tinyint(4) NOT NULL,
  `cond_desc` text,
  `sql_data` text,
  `use_cron` tinyint(4) NOT NULL,
  `period` int(11) DEFAULT NULL,
  `add_groups` text,
  `del_groups` text,
  `send_letter` tinyint(4) NOT NULL,
  `subject` varchar(255) DEFAULT NULL,
  `letter` longtext,
  `delivery_id` int(11) DEFAULT NULL,
  `unsubscribe` text,
  `create_date` int(11) NOT NULL,
  `next_action` int(11) DEFAULT NULL,
  `send_sms` tinyint(4) NOT NULL DEFAULT '0',
  `message` text,
  PRIMARY KEY (`id`),
  KEY `type` (`type`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course` (
  `course_id` int(11) NOT NULL AUTO_INCREMENT,
  `cat_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `title` varchar(255) NOT NULL,
  `meta_desc` varchar(255) DEFAULT NULL,
  `meta_keys` varchar(255) DEFAULT NULL,
  `alias` varchar(255) NOT NULL,
  `course_desc` text,
  `short_desc` varchar(255) DEFAULT NULL,
  `cover` varchar(255) DEFAULT NULL,
  `img_alt` varchar(255) DEFAULT NULL,
  `padding` varchar(50) DEFAULT NULL,
  `auto_train` tinyint(4) NOT NULL COMMENT 'автотренинг, да/нет',
  `type_access_buy` tinyint(4) NOT NULL COMMENT 'тип ссылки при покупке доступа: 0 - нет, 1-страница заказа, 2 - лендинг, 3-своя ссылка ',
  `product_access` int(11) DEFAULT NULL COMMENT 'id продукта для покупки доступа',
  `link_access` varchar(255) DEFAULT NULL COMMENT 'ссылка для покупки доступа',
  `show_lessons_count` tinyint(4) NOT NULL COMMENT 'показывать кол-во уроков в курсе',
  `show_desc` tinyint(4) NOT NULL COMMENT 'показывать описание на странице списка уроков',
  `show_progress` tinyint(4) NOT NULL COMMENT 'показывать прогресс прохождения',
  `show_comments` tinyint(4) NOT NULL COMMENT 'показывать кол-во комментариев к урокам курса',
  `show_hits` tinyint(4) NOT NULL COMMENT 'показывать кол-во просмотров уроков',
  `show_pupil` tinyint(4) NOT NULL COMMENT 'показывать тех кто прошёл курс + количество',
  `show_begin` tinyint(4) NOT NULL DEFAULT '0',
  `sort` smallint(6) NOT NULL,
  `sort_less` int(11) NOT NULL COMMENT 'сортировка уроков',
  `prof_id` int(11) NOT NULL,
  `curators` text COMMENT 'id кураторов курса',
  `create_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `start_date` int(11) DEFAULT NULL,
  `end_date` int(11) DEFAULT NULL,
  `sertificate_id` int(11) NOT NULL,
  `status` tinyint(4) NOT NULL,
  `author_id` int(11) DEFAULT NULL,
  `free_lessons` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'кол-во бесплатных первых уроков',
  `show_in_main` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'показывать на главной курсов',
  `is_free` tinyint(4) NOT NULL DEFAULT '0',
  `button_text` varchar(60) DEFAULT NULL COMMENT 'текст на кнопке купить',
  `view_desc` text COMMENT 'вариант просмотра описания курса: лендинг или ссылка',
  `access_type` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'тип доступа к курсу:0 - не выбран, 1-по группе, 2 по подписке, 9 - свободный	',
  `groups` text COMMENT 'список групп доступа',
  `access` text COMMENT 'список планов для доступа',
  PRIMARY KEY (`course_id`),
  KEY `index` (`cat_id`),
  KEY `is_free` (`is_free`)
) ENGINE=InnoDB AUTO_INCREMENT=15 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_answers` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `parent_id` int(11) NOT NULL,
  `course_id` int(11) DEFAULT NULL,
  `lesson_id` int(11) NOT NULL,
  `user_id` int(11) NOT NULL,
  `status` int(11) NOT NULL COMMENT '1 - отвечен, 0 - не отвечено',
  `date` int(11) NOT NULL,
  `body` longtext NOT NULL,
  `attach` text,
  PRIMARY KEY (`id`),
  KEY `lesson` (`lesson_id`),
  KEY `parent` (`parent_id`),
  KEY `lesson_id` (`lesson_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_category` (
  `cat_id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `alias` varchar(255) NOT NULL,
  `title` varchar(255) NOT NULL,
  `meta_desc` varchar(255) DEFAULT NULL,
  `meta_keys` varchar(255) DEFAULT NULL,
  `cat_desc` text,
  `cover` varchar(255) DEFAULT NULL,
  `img_alt` varchar(255) DEFAULT NULL,
  `status` int(11) NOT NULL,
  PRIMARY KEY (`cat_id`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_lesson_map` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `lesson_id` int(11) NOT NULL,
  `course_id` int(11) NOT NULL,
  `status` int(11) NOT NULL COMMENT '0 - не проверен, 1 - зачёт, 9 - автозачёт для крона',
  `date` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `user` (`user_id`),
  KEY `lesson` (`lesson_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_lessons` (
  `lesson_id` int(11) NOT NULL AUTO_INCREMENT,
  `course_id` int(11) NOT NULL,
  `block_id` int(11) DEFAULT NULL,
  `name` varchar(255) NOT NULL,
  `title` varchar(255) NOT NULL,
  `meta_desc` varchar(255) DEFAULT NULL,
  `meta_keys` varchar(255) DEFAULT NULL,
  `alias` varchar(255) NOT NULL,
  `less_desc` text,
  `cover` varchar(255) DEFAULT NULL,
  `img_alt` varchar(255) DEFAULT NULL,
  `access_type` varchar(35) NOT NULL,
  `access` text,
  `groups` text,
  `content` longtext NOT NULL,
  `video_urls` text,
  `audio_urls` text,
  `attach` text,
  `attach_title` varchar(255) DEFAULT NULL COMMENT 'Заголовок для вложения',
  `dopmat` text,
  `task_type` int(11) DEFAULT NULL COMMENT '0 - нет задания, 1 - без проверки, 2 - авто, 3 - ручная',
  `task` text,
  `task_time` int(11) DEFAULT NULL COMMENT 'время задержки для автопроверки, минуты',
  `allow_comments` int(11) NOT NULL,
  `sort` int(11) NOT NULL,
  `show_comments` int(11) NOT NULL COMMENT 'показывать иконку и кол-во комментариев',
  `show_hits_count` int(11) NOT NULL COMMENT 'показывать кол-во просмотров',
  `create_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `public_date` int(11) NOT NULL,
  `end_date` int(11) NOT NULL,
  `rating` int(11) NOT NULL,
  `hits` int(11) NOT NULL,
  `type_access_buy` int(11) NOT NULL COMMENT 'тип ссылки при покупке доступа: 0 - как в курсе, 1-страница заказа, 2 - лендинг, 3-своя ссылка ',
  `product_access` int(11) DEFAULT NULL COMMENT 'id продукта для покупки доступа',
  `link_access` varchar(255) DEFAULT NULL COMMENT 'ссылка для покупки доступа',
  `status` int(11) NOT NULL,
  `custom_code` text,
  `duration` int(11) DEFAULT NULL COMMENT 'продолжительность в минутах',
  `timing` smallint(6) NOT NULL DEFAULT '0' COMMENT 'открытие по расписанию',
  `timing_period` varchar(5) DEFAULT NULL COMMENT 'период для задержки: часы, дни, недели',
  `custom_code_up` text COMMENT 'произвольный код в начале урока',
  PRIMARY KEY (`lesson_id`),
  KEY `course` (`course_id`)
) ENGINE=InnoDB AUTO_INCREMENT=74 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_lessons_blocks` (
  `block_id` int(11) NOT NULL AUTO_INCREMENT,
  `course_id` int(11) NOT NULL,
  `sort` int(11) DEFAULT NULL,
  `block_name` varchar(255) NOT NULL,
  PRIMARY KEY (`block_id`)
) ENGINE=InnoDB AUTO_INCREMENT=31 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_mat_cats` (
  `cat_id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  PRIMARY KEY (`cat_id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_materials` (
  `mat_id` int(11) NOT NULL AUTO_INCREMENT,
  `file` text,
  `name` varchar(255) NOT NULL,
  `create_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `cat_id` int(11) NOT NULL,
  PRIMARY KEY (`mat_id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_professions` (
  `prof_id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `alias` varchar(255) NOT NULL,
  `title` varchar(255) NOT NULL,
  `prof_desc` text,
  `meta_desc` varchar(255) DEFAULT NULL,
  `meta_keys` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`prof_id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#course_sertificate` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `file` varchar(255) NOT NULL,
  `create_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `status` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#cron_logs` (
  `jobs_cron` varchar(20) DEFAULT NULL COMMENT 'Тип задания в кроне',
  `last_run` int(11) DEFAULT NULL COMMENT 'Последнее время выполнения задания',
  `jobs_error` tinyint(1) DEFAULT '0' COMMENT 'Ошибки в заданиях крона',
  `text_error` text COMMENT 'Полное сообщение ошибки',
  UNIQUE KEY `jobs_cron` (`jobs_cron`,`jobs_error`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#email_delivery` (
  `delivery_id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `delivery_desc` text,
  `type` int(11) NOT NULL COMMENT 'тип: 1 - массовая, 2 - автосерия',
  `send_time` int(11) NOT NULL COMMENT 'время отправки',
  `create_date` int(11) NOT NULL,
  `sent` text NOT NULL COMMENT 'список адресатов',
  `ex_sent` text NOT NULL COMMENT 'список исключений',
  `count_letters` int(11) NOT NULL COMMENT 'подсчёт кол-ва получателей для массовой',
  `confirmation` int(11) NOT NULL COMMENT 'подтверждение: 0 - нет, 1- всегда, 2 - только через форму',
  `confirm_subject` varchar(255) DEFAULT NULL COMMENT 'тема письма подтверждения',
  `confirm_body` longtext COMMENT 'текст письма подтверждения',
  `after_confirm_text` longtext COMMENT 'текст после подтверждения',
  `count_bad` int(11) NOT NULL DEFAULT '0' COMMENT 'подсчёт кол-ва плохих получателей',
  `bad_list_id` text CHARACTER SET utf8mb4 COMMENT 'список ID юзеров с плохими email`ами',
  PRIMARY KEY (`delivery_id`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#email_letter` (
  `letter_id` int(11) NOT NULL AUTO_INCREMENT,
  `delivery_id` int(11) NOT NULL,
  `send_time` int(11) NOT NULL,
  `subject` varchar(255) NOT NULL,
  `body` longtext NOT NULL,
  `target` longtext COMMENT 'описание задачи письма',
  `status` int(11) NOT NULL,
  `create_date` int(11) NOT NULL,
  PRIMARY KEY (`letter_id`),
  KEY `delivery` (`delivery_id`),
  KEY `send` (`send_time`)
) ENGINE=InnoDB AUTO_INCREMENT=6 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#email_reasons` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `delivery_id` int(11) NOT NULL DEFAULT '0' COMMENT 'id рассылки',
  `email` varchar(255) NOT NULL,
  `reason` longtext,
  `time` int(11) NOT NULL,
  `type` varchar(15) NOT NULL COMMENT 'тип отписки',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#email_subs_map` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `delivery_id` int(11) NOT NULL,
  `email` varchar(255) NOT NULL,
  `subs_name` varchar(55) DEFAULT NULL,
  `subs_time` int(11) NOT NULL,
  `subs_key` varchar(255) NOT NULL,
  `confirmed` int(11) NOT NULL COMMENT '0 или время в unix',
  `cancelled` int(11) NOT NULL COMMENT '0 или время в unix',
  `spam` int(11) NOT NULL COMMENT '0 или время в unix',
  `phone` varchar(32) DEFAULT NULL COMMENT 'номер телефона',
  PRIMARY KEY (`id`),
  KEY `delivery` (`delivery_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#email_task` (
  `task_id` int(11) NOT NULL AUTO_INCREMENT,
  `delivery_id` int(11) NOT NULL,
  `letter_id` int(11) NOT NULL,
  `email` varchar(255) NOT NULL,
  `send_time` int(11) NOT NULL,
  `status` int(11) NOT NULL,
  `letter` text,
  PRIMARY KEY (`task_id`),
  KEY `delivery` (`delivery_id`),
  KEY `letter` (`letter_id`),
  KEY `time` (`send_time`),
  KEY `status` (`status`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#email_task_log` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `letter_id` int(11) NOT NULL,
  `delivery_id` int(11) NOT NULL,
  `email` varchar(255) NOT NULL,
  `date_time` int(11) NOT NULL,
  `action` varchar(255) NOT NULL COMMENT 'send - отправлено, error - ошибка',
  `descript` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `letter` (`letter_id`),
  KEY `delivery` (`delivery_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#expertsender` (
  `product_id` int(11) NOT NULL AUTO_INCREMENT,
  `rspndr_order` int(11) DEFAULT NULL COMMENT 'тип подписки при оформлении заказа',
  `rspndr_pay` int(11) DEFAULT NULL COMMENT 'тип подписки при оплате заказа',
  PRIMARY KEY (`product_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#extensions` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(50) NOT NULL,
  `title` varchar(50) NOT NULL,
  `type` varchar(20) NOT NULL COMMENT 'тип расширения',
  `enable` int(11) NOT NULL,
  `params` text,
  `link` varchar(255) NOT NULL COMMENT 'Ссылка на настройки',
  `version` varchar(20) DEFAULT NULL COMMENT 'версия',
  `update_url` varchar(255) DEFAULT NULL COMMENT 'url обновления',
  PRIMARY KEY (`id`),
  UNIQUE KEY `name` (`name`),
  UNIQUE KEY `name_2` (`name`)
) ENGINE=InnoDB AUTO_INCREMENT=29 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#feedback` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `email` varchar(255) NOT NULL,
  `text` longtext,
  `phone` varchar(20) DEFAULT NULL,
  `field1` varchar(255) DEFAULT NULL,
  `field2` varchar(255) DEFAULT NULL,
  `field3` varchar(255) DEFAULT NULL,
  `form_id` int(11) DEFAULT '0',
  `comment` text,
  `create_date` int(11) NOT NULL,
  `status` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#feedback_forms` (
  `form_id` int(11) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) NOT NULL,
  `form_desc` varchar(255) DEFAULT NULL,
  `default_form` int(11) DEFAULT NULL,
  `params` text,
  `hits` int(11) NOT NULL DEFAULT '0',
  `status` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`form_id`)

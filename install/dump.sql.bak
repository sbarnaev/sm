  `autocheck_time` smallint(6) DEFAULT NULL,
  `access_type` tinyint(4) NOT NULL DEFAULT '2' COMMENT 'что нужно для открытия доступа: 1 - прохождение теста, 2 - выполнение задания, 3 - тест и задание',
  `check_status_type` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'статус проверки: 1 - зачет, 2 - баллы',
  `access_time` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'время для открытия доступа: 1 - сразу, 2 - на следующий день, 3 - через X дней, 4 - дождаться дня недели',
  `access_time_days` smallint(4) DEFAULT NULL COMMENT 'количество дней для открытия доступа',
  `access_time_weekday` tinyint(4) DEFAULT NULL COMMENT 'количество недель для открытия доступа',
  `show_upload_file` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'отображать поле для загрузки файла',
  `show_work_link` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'отображать поле для добавления ссылки',
  `hint` text COMMENT 'подсказка наставнику',
  `completed_on_time` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'действие, если задание выполнено вовремя: 0 - ничего, 1 - начислить баллы, 2 - отправить сообщение, 3 - добавить в группу',
  `not_completed_on_time` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'действие, если задание не выполнено вовремя: 0 - ничего, 1 - снять баллы, 2 - отправить сообщение, 3 - удалить из группы',
  `completed_time_add_group` text CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci COMMENT 'добавление в группу при выполеннии задания',
  `completed_time_del_group` text CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci COMMENT 'удаление из группы при выполеннии задания',
  PRIMARY KEY (`task_id`),
  UNIQUE KEY `lesson_id` (`lesson_id`)
) ENGINE=InnoDB AUTO_INCREMENT=50 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#training_test` (
  `test_id` int(11) NOT NULL AUTO_INCREMENT,
  `task_id` int(11) NOT NULL,
  `lesson_id` int(11) NOT NULL,
  `test_desc` text,
  `finish` int(11) NOT NULL DEFAULT '0' COMMENT 'количество баллов, которое нужно набрать',
  `test_try` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'кол-во попыток сдачи теста',
  `test_time` int(11) NOT NULL DEFAULT '0' COMMENT 'время на прохождение теста, минуты',
  `show_questions_count` tinyint(4) NOT NULL DEFAULT '1' COMMENT 'кол-во выводимых вопросов',
  `help_hint_success` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'показывать расшифровку если сдал',
  `help_hint_fail` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'показывать расшифровку если НЕ сдал',
  `ratings` text CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci COMMENT 'оценки за прохождение теста',
  PRIMARY KEY (`test_id`),
  UNIQUE KEY `lesson_id` (`lesson_id`),
  KEY `task_id` (`task_id`)
) ENGINE=InnoDB AUTO_INCREMENT=50 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#training_test_options` (
  `option_id` int(11) NOT NULL AUTO_INCREMENT,
  `quest_id` int(11) NOT NULL DEFAULT '0',
  `title` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci NOT NULL,
  `value` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `sort` tinyint(4) NOT NULL,
  `valid` tinyint(4) NOT NULL,
  `points` smallint(6) NOT NULL DEFAULT '0',
  `cover` varchar(255) DEFAULT NULL,
  PRIMARY KEY (`option_id`),
  KEY `quest_id` (`quest_id`)
) ENGINE=InnoDB AUTO_INCREMENT=77 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#training_test_results` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `test_id` int(11) NOT NULL,
  `lesson_id` int(11) NOT NULL DEFAULT '0',
  `user_id` int(11) NOT NULL,
  `quest_id` int(11) NOT NULL,
  `result` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `date` int(11) NOT NULL,
  `points` smallint(6) NOT NULL DEFAULT '0' COMMENT 'сколько баллов начислено',
  `is_valid` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'дан правильный ответ',
  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`),
  KEY `test_id` (`test_id`)
) ENGINE=InnoDB AUTO_INCREMENT=44 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

CREATE TABLE `#PREFIX#training_user_map` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `lesson_id` int(11) NOT NULL,
  `training_id` int(11) NOT NULL,
  `open` int(11) DEFAULT NULL,
  `date` int(11) DEFAULT NULL,
  `get_answer` int(11) DEFAULT NULL,
  `status` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'LESSON_STARTED = 0, HOMEWORK_SUBMITTED = 1, HOMEWORK_DECLINE = 2, HOMEWORK_ACCEPTED = 3, HOMEWORK_AUTOCHECK = 4',
  `count_answer` tinyint(4) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`),
  KEY `lesson_id` (`lesson_id`),
  KEY `training_id` (`training_id`)
) ENGINE=InnoDB AUTO_INCREMENT=71 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#training_user_visits` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `training_id` int(11) NOT NULL,
  `section_id` int(11) NOT NULL DEFAULT '0',
  `lesson_id` int(11) NOT NULL DEFAULT '0',
  `date` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`),
  KEY `training_id` (`training_id`),
  KEY `lesson_id` (`lesson_id`),
  KEY `section_id` (`section_id`)
) ENGINE=InnoDB AUTO_INCREMENT=72 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#training_wiget_settings` (
  `setting_id` int(11) NOT NULL AUTO_INCREMENT,
  `params` text,
  PRIMARY KEY (`setting_id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#user_blacklist` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `email` varchar(255) NOT NULL,
  `date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#user_groups` (
  `group_id` int(11) NOT NULL AUTO_INCREMENT,
  `group_name` varchar(50) NOT NULL,
  `group_title` varchar(50) NOT NULL,
  `group_desc` varchar(255) DEFAULT NULL,
  `create_date` int(11) NOT NULL,
  `del_tg_chats` text COMMENT 'ID или название чатов Telegram для удаления из них пользователей при удалении группы',
  PRIMARY KEY (`group_id`)
) ENGINE=InnoDB AUTO_INCREMENT=23 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#user_groups_map` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `group_id` int(11) NOT NULL,
  `date` int(11) NOT NULL,
  PRIMARY KEY (`id`),
  KEY `user` (`user_id`),
  KEY `group` (`group_id`)
) ENGINE=InnoDB AUTO_INCREMENT=123 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#users` (
  `user_id` int(11) NOT NULL AUTO_INCREMENT,
  `user_name` varchar(255) DEFAULT NULL,
  `login` varchar(20) DEFAULT NULL,
  `email` varchar(255) NOT NULL,
  `phone` varchar(25) DEFAULT NULL,
  `confirm_phone` varchar(32) DEFAULT NULL COMMENT 'подтвержденный номер телефона',
  `city` varchar(50) DEFAULT NULL,
  `address` varchar(255) DEFAULT NULL,
  `zipcode` varchar(15) DEFAULT NULL,
  `note` text,
  `pass` varchar(255) NOT NULL,
  `role` varchar(10) NOT NULL,
  `is_partner` tinytext,
  `is_client` tinyint(4) DEFAULT NULL,
  `is_subs` tinyint(4) DEFAULT NULL COMMENT 'подписчик email рассылки',
  `is_author` tinyint(4) DEFAULT NULL,
  `is_curator` tinyint(4) DEFAULT NULL COMMENT 'куратор курса?',
  `enter_time` int(11) DEFAULT NULL COMMENT 'дата фиксации системой',
  `reg_date` int(11) DEFAULT NULL COMMENT 'дата регистрации',
  `enter_method` varchar(20) DEFAULT NULL COMMENT 'метод входа в систему (free, paid и т.д.)',
  `refer` varchar(255) DEFAULT NULL,
  `channel_id` int(11) DEFAULT NULL COMMENT 'id канала привлечения',
  `recovery` int(11) DEFAULT NULL COMMENT 'для восстановления пароля',
  `reg_key` varchar(255) DEFAULT NULL,
  `last_visit` int(11) DEFAULT NULL,
  `from_id` int(11) DEFAULT NULL COMMENT 'от кого пришёл, субпартнёр',
  `status` tinyint(4) NOT NULL COMMENT '6 - чёрный список',
  `points` int(11) DEFAULT NULL COMMENT 'баллы юзера',
  `surname` varchar(30) DEFAULT NULL COMMENT 'фамилия',
  `nick_telegram` varchar(50) DEFAULT NULL,
  `nick_instagram` varchar(50) DEFAULT NULL,
  `vk_url` varchar(255) DEFAULT NULL,
  `sex` varchar(7) DEFAULT NULL,
  `level` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'уровень юзера',
  `bith_day` tinyint(4) DEFAULT NULL,
  `bith_month` tinyint(4) DEFAULT NULL,
  `bith_year` smallint(6) DEFAULT NULL,
  `photo_url` text,
  `spec_aff` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'особый режим партнёрки',
  PRIMARY KEY (`user_id`),
  KEY `entertime` (`enter_time`),
  KEY `email` (`email`)
) ENGINE=InnoDB AUTO_INCREMENT=164 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#users_spec_aff` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `product_id` int(11) NOT NULL,
  `type` tinyint(4) NOT NULL,
  `float_scheme` text NOT NULL,
  `comiss` smallint(6) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `user_id` (`user_id`)
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#widgets` (
  `widget_id` int(11) NOT NULL AUTO_INCREMENT,
  `widget_type` varchar(20) NOT NULL,
  `widget_title` varchar(40) NOT NULL,
  `widget_desc` text,
  `position` varchar(30) DEFAULT NULL,
  `suffix` varchar(15) DEFAULT NULL,
  `show_header` int(11) NOT NULL DEFAULT '1',
  `header` varchar(128) NOT NULL DEFAULT '' COMMENT 'Заголовок виджета',
  `show_subheader` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Показывать подзаголовок виджета',
  `subheader` varchar(128) NOT NULL DEFAULT '' COMMENT 'Подзаголовок виджета',
  `show_right_button` tinyint(4) NOT NULL DEFAULT '0' COMMENT 'Показывать кнопку справа',
  `right_button_name` varchar(128) NOT NULL DEFAULT '' COMMENT 'Название кнопки справа',
  `right_button_link` varchar(256) NOT NULL DEFAULT '' COMMENT 'Ссылка кнопки справа',
  `params` mediumtext,
  `sort` int(11) NOT NULL,
  `affix` int(11) NOT NULL,
  `private` int(11) NOT NULL COMMENT 'приватный, только для зарегистрированных',
  `status` int(11) NOT NULL,
  `create_date` int(11) NOT NULL,
  `show_for_course` text,
  `show_for_training` text,
  PRIMARY KEY (`widget_id`)
) ENGINE=InnoDB AUTO_INCREMENT=22 DEFAULT CHARSET=utf8;

CREATE TABLE `#PREFIX#widgets_map` (
  `widget_id` int(11) NOT NULL,
  `page` varchar(50) NOT NULL,
  PRIMARY KEY (`widget_id`,`page`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

